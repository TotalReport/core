  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^29.7.0",
    "@swc/jest": "^0.2.36",
    "@total-report/typescript-config": "workspace:*",
    "@ts-rest/core": "^3.45.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "typescript": "5.5.3"
  }
